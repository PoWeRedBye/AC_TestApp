<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dinoExample.maxim_ozarovskiy.artcoraltestapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/unnamed"
        android:label="@string/app_name"
        android:roundIcon="@drawable/unnamed"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.maxim_ozarovskiy.artcoraltestapp.activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.maxim_ozarovskiy.artcoraltestapp.activities.LoginActivity" />
        <activity android:name="com.example.maxim_ozarovskiy.artcoraltestapp.activities.MainActivity" />
        <activity android:name="com.example.maxim_ozarovskiy.artcoraltestapp.activities.RegisterActivity" />
        <activity android:name="com.example.maxim_ozarovskiy.artcoraltestapp.activities.CreateDinoActivity" />
    </application>

</manifest>